<form action="{{ path('search_article') }}" method="GET">
    <input type="search" name="search">
    <input type="submit" value="Rechercher">
</form>


{% for article in articles %}
<div>
    <h1>{{ article.title }}</h1>
    <h6>{{ article.date|date('d/m/Y')  }}</h6>
    <div>
        <img src="{{ asset ('uploads/admin/articles/') ~ article.image}}" alt="">
    </div>
    <p> {{ article.content }}</p>
    {% endfor %}
</div>