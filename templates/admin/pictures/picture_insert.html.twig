{% extends 'admin/common/base.html.twig' %}

{% block content %}

    <section class="containerInsert">
        <div class="singleCatInsert xLarge-3 large-3 medium-6 small-9">
            <div class="formInsert">
                <div>
                    <p>Ajouter une photo</p>
                </div>
                {{ form_start(formPicture) }}
                <div class="labelInsert">
                    {{ form_row(formPicture.description) }}
                    {{ form_row(formPicture.image) }}
                </div>
                <div class="btnInsert">
                    {{ form_widget(formPicture.submit) }}
                </div>
                {{ form_end(formPicture) }}
            </div>
        </div>
        <div class="returnSmall">
            <a href="{{ path ('admin_pictures_list') }}">
                <i class="fa fa-reply"></i>
            </a>
        </div>
    </section>

{% endblock %}